{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "advent-of-code",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "interop",
      "type": "dev"
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".mjs",
  "bs-dependencies": [
    "@rescript/core",
    "rescript-nodejs",
    "rationale",
    "rescript-bag",
    "@dsiu/rescript-stdlib-fp",
    "@dsiu/rescript-graphology"
  ],
  "bs-dev-dependencies": [
    "@glennsl/rescript-jest"
  ],
  "warnings": {
    "error": "+101"
  },
  "namespace": true,
  "refmt": 3
}
