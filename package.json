{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "yarn@4.5.3",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "github:dsiu/rescript-jest#rescript-v12",
    "@swc/core": "^1.9.3",
    "@swc/jest": "^0.2.37",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "rescript": "^12.0.0-alpha.5"
  },
  "jest": {
    "testMatch": [
      "**/*_Test.(js|ts|jsx|tsx|mjs)"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@rescript/core|@glennsl/rescript-jest|@ryyppy/rescript-promise|re-fp|rationale|rescript-bag|@resinfo/parser|rescript-js|js-array2-ex|tablecloth-rescript|@dsiu/rescript-stdlib-fp|@dsiu/rescript-graphology|@greenlabs/garter|relude|bs-bastet|relude-parse|)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@dsiu/rescript-graphology": "https://github.com/dsiu/rescript-graphology",
    "@dsiu/rescript-stdlib-fp": "git@github.com:dsiu/rescript-stdlib-fp.git#commit=8ecf5d866ed1f2512051b76657c7781162bdc61a",
    "@jmagaram/rescript-extras": "^4.1.0",
    "@jmagaram/rescript-seq": "^4.4.1",
    "@rescript/core": "^1.6.1",
    "debug": "^4.3.7",
    "ms": "^2.1.3",
    "rationale": "^0.2.0",
    "rescript-bag": "git@github.com:dsiu/rescript-bag.git#420eeec4594f6bfb3ecae47f33e5d64005416353",
    "rescript-nodejs": "^16.1.0",
    "rescript-vector": "^0.4.0"
  }
}
