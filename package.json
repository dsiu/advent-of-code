{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.1",
    "esbuild-jest": "^0.5.0",
    "jest": "^28.1.2",
    "rescript": "^9.1.4"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/**/*.test.(js|jsx|ts|tsx)",
      "<rootDir>/**/*_Test.bs.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "esbuild-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@glennsl/rescript-jest|@ryyppy/rescript-promise|rationale|rescript-ocamlgraph|@resinfo/parser)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@resinfo/parser": "^2.0.0-alpha",
    "debug": "^4.3.4",
    "rationale": "^0.2.0",
    "re-fp": "github:snatvb/re-fp",
    "rescript-js": "^0.7.0",
    "rescript-ocamlgraph": "github:dsiu/rescript-ocamlgraph#main",
    "rescript-vector": "^0.4.0"
  }
}
