{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "yarn@4.5.3",
  "scripts": {
    "build": "rewatch build .",
    "watch": "rewatch watch .",
    "clean": "rewatch clean .",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "github:dsiu/rescript-jest#rescript-v12",
    "@swc/core": "^1.9.3",
    "@swc/jest": "^0.2.37",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "rescript": "^12.0.0-alpha.5"
  },
  "jest": {
    "testMatch": [
      "**/*_Test.(js|ts|jsx|tsx|mjs)"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@rescript/core|@glennsl/rescript-jest|@ryyppy/rescript-promise|re-fp|rescript-bag|@resinfo/parser|rescript-js|js-array2-ex|tablecloth-rescript|@dsiu/rescript-stdlib-fp|@dsiu/rescript-graphology|@greenlabs/garter|relude|bs-bastet|relude-parse|)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@dsiu/rescript-graphology": "git@github.com:dsiu/rescript-graphology.git#ffa1e000c26d72e7e5967568fbbc2104e6b71312",
    "@dsiu/rescript-stdlib-fp": "git@github.com:dsiu/rescript-stdlib-fp.git#commit=da5b78f372543dc0282e6a13536951a70df7cd1f",
    "@jmagaram/rescript-extras": "^4.1.0",
    "@jmagaram/rescript-seq": "^4.4.1",
    "@rolandpeelen/rewatch": "^1.0.9",
    "debug": "^4.3.7",
    "ms": "^2.1.3",
    "rescript-bag": "git@github.com:dsiu/rescript-bag.git#27a1e8c8311ff35377a5e2192da24a9b22d49dba",
    "rescript-nodejs": "^16.1.0",
    "rescript-vector": "^0.4.0"
  }
}
