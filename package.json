{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "https://github.com/dsiu/rescript-jest#rescript-v11-curried-mode",
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.2.29",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rescript": "^11.0.0-rc.8"
  },
  "jest": {
    "testMatch": [
      "**/*_Test.(js|ts|jsx|tsx|mjs)"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@rescript/core|@glennsl/rescript-jest|@ryyppy/rescript-promise|re-fp|rationale|rescript-bag|rescript-ocamlgraph|@resinfo/parser|rescript-js|js-array2-ex|tablecloth-rescript|@dsiu/rescript-stdlib-fp|@greenlabs/garter|relude|bs-bastet|relude-parse|)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@dsiu/rescript-stdlib-fp": "https://github.com/dsiu/rescript-stdlib-fp#commit=05028ae0db4b97bc033a9e7ca435fc8274c6304d",
    "@greenlabs/garter": "https://github.com/dsiu/garter#rescript-v11-curried-mode",
    "@jmagaram/rescript-extras": "^3.0.0",
    "@jmagaram/rescript-seq": "^3.0.1",
    "@rescript/core": "https://github.com/rescript-association/rescript-core#main",
    "@resinfo/parser": "^2.0.0-alpha",
    "bs-bastet": "https://github.com/dsiu/rescript-bastet#rescript-v11-curried-mode",
    "debug": "^4.3.4",
    "js-array2-ex": "^0.0.4",
    "ms": "^2.1.3",
    "rationale": "^0.2.0",
    "relude": "https://github.com/dsiu/rescript-relude#rescript-v11-curried-mode",
    "relude-parse": "https://github.com/dsiu/rescript-relude-parse#rescript-v11-curried-mode",
    "rescript-bag": "https://github.com/dsiu/rescript-bag#main",
    "rescript-js": "latest",
    "rescript-ocamlgraph": "https://github.com/dsiu/rescript-ocamlgraph#rescript-v10",
    "rescript-vector": "^0.4.0"
  }
}
