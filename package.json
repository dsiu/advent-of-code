{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.2",
    "@swc/core": "^1.2.244",
    "@swc/jest": "^0.2.23",
    "jest": "^29.0.1",
    "nodemon": "^2.0.20",
    "rescript": "^10.0.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/**/*.test.(js|mjs|jsx|ts|tsx)",
      "<rootDir>/**/*_Test.mjs"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@glennsl/rescript-jest|@ryyppy/rescript-promise|re-fp|rationale|rescript-bag|rescript-ocamlgraph|@resinfo/parser|rescript-js|js-array2-ex|tablecloth-rescript|)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@resinfo/parser": "^2.0.0-alpha",
    "bs-bastet": "github:dsiu/bastet#rescript-v10",
    "debug": "^4.3.4",
    "js-array2-ex": "^0.0.4",
    "ms": "^2.1.3",
    "rationale": "^0.2.0",
    "re-fp": "github:snatvb/re-fp",
    "relude": "github:dsiu/relude#rescript-v10",
    "rescript-bag": "github:dsiu/rescript-bag#main",
    "rescript-js": "latest",
    "rescript-ocamlgraph": "github:dsiu/rescript-ocamlgraph#rescript-v10",
    "rescript-vector": "^0.4.0",
    "tablecloth-rescript": "github:darklang/tablecloth-rescript#main"
  }
}
