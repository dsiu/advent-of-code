{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.10.0",
    "@swc/core": "^1.3.69",
    "@swc/jest": "^0.2.26",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "rescript": "^10.1.4"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/**/*.test.(js|mjs|jsx|ts|tsx)",
      "<rootDir>/**/*_Test.mjs"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@glennsl/rescript-jest|@ryyppy/rescript-promise|re-fp|rationale|rescript-bag|rescript-ocamlgraph|@resinfo/parser|rescript-js|js-array2-ex|tablecloth-rescript|@dsiu/rescript-stdlib-fp|relude|bs-bastet|relude-parse|)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@dsiu/rescript-stdlib-fp": "github:dsiu/rescript-stdlib-fp",
    "@jmagaram/rescript-extras": "^3.0.0",
    "@jmagaram/rescript-seq": "^3.0.1",
    "@rescript/core": "github:rescript-association/rescript-core",
    "@resinfo/parser": "^2.0.0-alpha",
    "bs-bastet": "github:dsiu/rescript-bastet#main",
    "debug": "^4.3.4",
    "js-array2-ex": "^0.0.4",
    "ms": "^2.1.3",
    "rationale": "^0.2.0",
    "re-fp": "github:snatvb/re-fp",
    "relude": "github:dsiu/rescript-relude#main",
    "relude-parse": "github:dsiu/rescript-relude-parse#main",
    "rescript-bag": "github:dsiu/rescript-bag#main",
    "rescript-js": "latest",
    "rescript-ocamlgraph": "github:dsiu/rescript-ocamlgraph#rescript-v10",
    "rescript-vector": "^0.4.0",
    "tablecloth-rescript": "github:darklang/tablecloth-rescript#main"
  }
}
