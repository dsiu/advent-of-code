{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "https://github.com/dsiu/rescript-jest#rescript-v11-curried-mode",
    "@swc/core": "^1.3.105",
    "@swc/jest": "^0.2.31",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "rescript": "^11.1.0-rc.7"
  },
  "jest": {
    "testMatch": [
      "**/*_Test.(js|ts|jsx|tsx|mjs)"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@rescript/core|@glennsl/rescript-jest|@ryyppy/rescript-promise|re-fp|rationale|rescript-bag|@resinfo/parser|rescript-js|js-array2-ex|tablecloth-rescript|@dsiu/rescript-stdlib-fp|@dsiu/rescript-graphology|@greenlabs/garter|relude|bs-bastet|relude-parse|)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@dsiu/rescript-graphology": "https://github.com/dsiu/rescript-graphology#commit=62847f95c76e6facab8a6c9b9e9d1c308fb4480e",
    "@dsiu/rescript-stdlib-fp": "https://github.com/dsiu/rescript-stdlib-fp#commit=93e06faa43b7de9403d14515cd24cb421c3ac798",
    "@greenlabs/garter": "https://github.com/dsiu/garter#rescript-v11-curried-mode",
    "@jmagaram/rescript-extras": "^3.0.0",
    "@jmagaram/rescript-seq": "^3.0.1",
    "@rescript/core": "^1.2.0",
    "@resinfo/parser": "https://github.com/dsiu/rescript-parser#commit=02b0e3d1c1ea00a7c0a9f2091c89c674952bef87",
    "bs-bastet": "https://github.com/dsiu/rescript-bastet#rescript-v11-uncurried-wip",
    "debug": "^4.3.4",
    "js-array2-ex": "^0.0.4",
    "ms": "^2.1.3",
    "rationale": "^0.2.0",
    "relude": "https://github.com/dsiu/rescript-relude#rescript-v11-uncurried-wip",
    "relude-parse": "https://github.com/dsiu/rescript-relude-parse#rescript-v11-uncurried-wip",
    "rescript-bag": "https://github.com/dsiu/rescript-bag#main",
    "rescript-nodejs": "^16.1.0",
    "rescript-vector": "^0.4.0"
  }
}
