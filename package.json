{
  "name": "advent-of-code",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "reformat": "./scripts/reformat.sh"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.1",
    "@swc/core": "^1.2.218",
    "@swc/jest": "^0.2.22",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "rescript": "^9.1.4"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/**/*.test.(js|mjs|jsx|ts|tsx)",
      "<rootDir>/**/*_Test.mjs"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@glennsl/rescript-jest|@ryyppy/rescript-promise|rationale|rescript-bag|rescript-ocamlgraph|@resinfo/parser|rescript-js)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
    "@resinfo/parser": "^2.0.0-alpha",
    "debug": "^4.3.4",
    "rationale": "^0.2.0",
    "re-fp": "github:snatvb/re-fp",
    "rescript-js": "^0.7.0",
    "rescript-ocamlgraph": "github:dsiu/rescript-ocamlgraph#main",
    "rescript-bag": "github:dsiu/rescript-bag#main",
    "rescript-vector": "^0.4.0"
  }
}
