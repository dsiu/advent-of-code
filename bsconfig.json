{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "advent-of-code",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "type": "dev"
    },
    {
      "dir": "interop",
      "type": "dev"
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".mjs",
  "bs-dependencies": [
    "@rescript/core",
    "rationale",
    "@ryyppy/rescript-promise",
    "rescript-js",
    "re-fp",
    "rescript-ocamlgraph",
    "rescript-bag",
    "@resinfo/parser",
    "bs-bastet",
    "relude",
    "relude-parse",
    "js-array2-ex",
    "tablecloth-rescript",
    "@dsiu/rescript-stdlib-fp"
  ],
  "bs-dev-dependencies": [
    "@glennsl/rescript-jest"
  ],
  "warnings": {
    "error": "+101"
  },
  "namespace": true,
  "refmt": 3
}
